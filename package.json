{
  "name": "firebase-chat",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0-alpha.6",
    "@material-ui/styles": "^4.0.0-alpha.6",
    "@types/jest": "24.0.11",
    "@types/node": "11.12.2",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "firebase": "^5.9.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "typescript": "3.4.1"
  },
  "scripts": {
    "start": "npm run build:firebase && run-p --race start:firebase:serve start:react",
    "start:react": "react-scripts start",
    "start:firebase": "npm run build:firebase && npm run start:firebase:serve",
    "start:firebase:serve": "firebase serve -p 4000",
    "build": "npm run build:react && npm run build:firebase",
    "build:react": "react-scripts build",
    "build:firebase": "npm --prefix='functions' run build",
    "lint": "tslint --project tsconfig.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:4000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tslint": "^5.14.0"
  }
}
